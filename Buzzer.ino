  // You can replace the code here to set it to a different song
  notes = OrdinaryPathNotes;
  noteLengths = OrdinaryPathNoteLengths;
  delays = OrdinaryPathDelays;
  // This must follow the boundary number of the respectively song STRICTLY, or some weird notes will be played.
  boundary = 388;
}

void loop() {
  for (int i=0; i<boundary*2-1; i++) {
    if (i % 2) {
      // Is Delay
      delay(pgm_read_word_near(&delays[i/2]));
    } else {
      // Is Note
      tone(TONE_PIN, pgm_read_word_near(&notes[i/2]), pgm_read_word_near(&noteLengths[i/2]));
    }
  }
}
